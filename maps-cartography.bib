@article{KivelsonCartographyAutocracyState1999,
  title = {Cartography, Autocracy and State Powerlessness: The Uses of Maps in Early Modern Russia},
  shorttitle = {Cartography, Autocracy and State Powerlessness},
  author = {Kivelson, Valerie A.},
  year = {1999},
  journal = {Imago Mundi},
  volume = {51},
  pages = {83-105},
  issn = {0308-5694},
  url = {http://www.jstor.org/stable/1151442},
  urldate = {2019-11-07},
  abstract = {Historians have focused much attention on the ways in which the rising monarchies of early modern Europe crushed or incorporated the peripheries of their territories into a uniform and unifying state system. Examination of nearly 500 Russian property-litigation maps from the second half of the seventeenth century demonstrates that elimination of local power bases need not accompany state-generated mapping programmes. On the contrary, a quite different and much more evenly balanced process of negotiation and interplay between local and central authority facilitated the construction of an extensive, intrusive state. The maps, together with the legal documents that accompany them, also demonstrate that the Romanov regime-often considered the highest expression of early modern absolutism-frequently found itself entangled in its own laws and regulations. The overview presented in this paper of Russian local maps and the legal context which gave rise to them allows for a re-reading of centre-periphery relationships in the Russian tsardom and suggests a new understanding of the rise of autocratic states in general. / Les historiens ont concentré une particulière attention sur les moyens par lesquels les monarchies du début de l'Europe moderne dans leur essor ont écrasé ou annexé des terres limitrophes de leurs propres possessions en les incluant dans des organisations d'uniformisation et d'unification. En étudiant près de 500 cartes russes de contestations de propriétés de la seconde moitié du dix-septième siècle, on démontre que l'activité cartographique d'Etat n'a pas nécessairement amenée à la destruction des pouvoirs locaux. Au contraire, c'est par une méthode toute différente et beaucoup plus régulièrement équilibrée de tractations réciproques entre les autorités locales et centrales que s'opérait plus facilement la formation d'un état par extension et ingérence. Les cartes ainsi que les documents juridiques qui les accompagnent, prouvent aussi que le régime des Romanov-souvent représenté comme la réalisation la plus nette des débuts de l'absolutisme moderne-s'est trouvé souvent empêtré dans ses propres lois et règlements. Le survol présenté dans cet article sur les cartes locales russes et le contexte juridique qui fut à leur origine permet une relecture des rapports entre le centre et la périphérie à l'intérieur de l'empire tsariste et suggère un nouvelle compréhension du développement des états autocrates en général. / Historiker haben sich mit großer Aufmerksamkeit der Frage gewidmet, auf welche Weise aufstrebende Monarchien im frühneuzeitlichen Europa ihre dezentralen Territorien in einheitliche oder vereinheitlichende Staatssysteme preßten oder eingliederten. Die Untersuchung von annähernd 500 russischen Besitzstreitigkeitskarten aus der zweiten Hälfte des 17. Jahrhunderts zeigt, daß offizielle Kartographie nicht notwendigerweise die Niederschlagung lokaler Macht voraussetzte. Der Aufbau eines ausgedehnten und immer stärker einfluss nehmenden Staates wurde vielmehr durch einen ausgewogenen Prozeß von Verhandlungen zwischen der zentralen Autorität und den lokalen Großgrundbesitzern begünstigt. Die Karten und die zugehörigen Prozeßakten beweisen, daß sich das Regime der Romanovs -häufig als der Inbegriff frühneuzeitlichen Absolutismus angesehen-oft selbst in den eigenen Gesetzen und Verordnungen gefangen sah. Der hier gebotene Überblick über russische Regionalkarten und das zu ihrer Entstehung führende rechtliche Umfeld ermöglicht die Neuinterpretation des Verhältnisses zwischen Zentrum und Peripherie im zaristischen Rußland und damit auch der Entwicklung autokratischer Staatswesen im allgemeinen.}
}

