@incollection{WoodMaps2009,
  title = {Maps},
  booktitle = {International Encyclopedia of Human Geography},
  author = {Wood, D. and Krygier, J.},
  editor = {Kitchin, Rob and Thrift, Nigel},
  year = {2009},
  month = jan,
  pages = {421-430},
  publisher = {Elsevier},
  address = {Oxford},
  doi = {10.1016/B978-008044910-4.00049-3},
  url = {http://www.sciencedirect.com/science/article/pii/B9780080449104000493},
  urldate = {2020-01-07},
  abstract = {Maps are artifacts that selectively link places to other kinds of things for the purpose of underwriting the reproduction (or contestation) of the social relations of power, and therefore are more readily exemplified than defined. Maps have become important comparatively recently, almost all of them having been made in the past hundred years, and their growing use parallels the rise of the modern state. Although maps were made before the rise of the modern state, they were only rarely made, appearing sparsely in cultural records everywhere in the world, and generally as facets of small-scale cosmological speculation and large-scale property control functions (a military mapping function appears uniquely early in China, and a coastal sailing function in the late middle ages in the Mediterranean basin). Maps as a way of advancing propositions about an open variety of spatial relations, at any scale, begin appearing only in the fifteenth century (in the twelfth century in China), and do not become common until the sixteenth century, when map production explodes around the world, and indigenous traditions from Japan, China, Europe, the New World, and elsewhere fuse into the synthesis we recognize today. The map’s ability to embody the abstraction of the modern state is salient.},
  isbn = {978-0-08-044910-4},
  langid = {english},
  keywords = {Early modern state,History of cartography,Map discourse function,Mapmaking,Maps,National identity,Property control function,Propositional character of the map},
  file = {/home/matt/Zotero/storage/PX44N5J5/wood_krygier-maps-2009.pdf;/home/matt/Zotero/storage/58XTPR6H/B9780080449104000493.html}
}

