@article{Matamoros-FernandezPlatformedracismmediation2017,
  title = {Platformed racism: the mediation and circulation of an Australian race-based controversy on Twitter, Facebook and YouTube},
  shorttitle = {Platformed racism},
  author = {Matamoros-Fernández, Ariadna},
  year = {2017},
